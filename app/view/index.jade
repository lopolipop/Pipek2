doctype html
html(lang="en", ng-app="application")
  head
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1")
    // The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags
    meta(name="description", content="")
    meta(name="author", content="")
    title= title
    // Bootstrap core CSS
    link(rel="stylesheet", href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css")
    link(rel="stylesheet", href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css")
    link(rel="stylesheet", href="/css/style.css", type="text/css")

  body
    header
      .container-fulid.clearfix
        .header-box
          .row
            .col-xs-1
              a(href="/")
                img.menu(src="/img/menu.png", alt="menu")
            .col-xs-1
              a(href="/")
                img.logo(src="/img/gableLogo.png", alt="logo")
            //- .col-xs-1
            //- .col-lg-5
            //-   form(action="/searchbox")
            //-     .searchbox
            //-       input(type="text", placeholder="Search..")
            //-       button
            //-         svg(viewbox='0 0 24 24', preserveaspectratio='xMidYMid meet', focusable='false')
            //-             path(d='M15.5 14h-.79l-.28-.27C15.41 12.59 16 11.11 16 9.5 16 5.91 13.09 3 9.5 3S3 5.91 3 9.5 5.91 16 9.5 16c1.61 0 3.09-.59 4.23-1.57l.27.28v.79l5 4.99L20.49 19l-4.99-5zm-6 0C7.01 14 5 11.99 5 9.5S7.01 5 9.5 5 14 7.01 14 9.5 11.99 14 9.5 14z')
    .container.searchform
      .col-lg-12
        .row
          .title
            | Search your next condo
        .row
          input(type="text", placeholder="Search condo name...")
          button
            svg(viewbox='0 0 24 24', preserveaspectratio='xMidYMid meet', focusable='false')
                path(d='M15.5 14h-.79l-.28-.27C15.41 12.59 16 11.11 16 9.5 16 5.91 13.09 3 9.5 3S3 5.91 3 9.5 5.91 16 9.5 16c1.61 0 3.09-.59 4.23-1.57l.27.28v.79l5 4.99L20.49 19l-4.99-5zm-6 0C7.01 14 5 11.99 5 9.5S7.01 5 9.5 5 14 7.01 14 9.5 11.99 14 9.5 14z')
        .row.selector
          .col-lg-3.left
            select.form-control.for
              option(value="all", selected, disabled) For
              option(value="all") All
              option(value="buy") Buy
              option(value="sell") Rent
          .col-lg-3.middle
            select.form-control.property
              option(value="all", selected, disabled) Property Types
              option(value="all") All
              option(value="condo") Condo
              option(value="house") House
          .col-lg-3.middle.province
            select.form-control
              option(value="all", selected, disabled) Province
              option(value="all") All
              option(value="all") Bangkok
              option(value="all") Chiang Mai
        
